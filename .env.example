# Symfony
APP_ENV=dev
APP_SECRET=change_me_to_a_random_32_chars_string

# Salesforce - JWT Bearer Flow Authentication
# Voir CLAUDE.md pour les instructions de configuration de la Connected App
SALESFORCE_INSTANCE_URL=https://yourinstance.salesforce.com
SALESFORCE_CLIENT_ID=your_consumer_key_from_connected_app
SALESFORCE_USERNAME=integration@yourcompany.com
SALESFORCE_PRIVATE_KEY_PATH=/path/to/salesforce_private_key.pem
# Production: https://login.salesforce.com | Sandbox: https://test.salesforce.com
SALESFORCE_AUDIENCE_URL=https://login.salesforce.com
